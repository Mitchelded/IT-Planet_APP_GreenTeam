<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiApp1.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiApp1"
    Shell.NavBarIsVisible="False"
    xmlns:view="clr-namespace:MauiApp1.View">

    
    <TabBar>
        <Tab Icon="home.svg" Title="Home">
            <ShellContent
            ContentTemplate="{DataTemplate local:PrewMainPage}"
                
            />
        </Tab>
        <Tab Icon="calendare.svg" Title="Calendare">
            <ShellContent
            ContentTemplate="{DataTemplate view:Calendare}"
            />
        </Tab>
        <Tab Icon="settings.svg" Title="Settings">
            <ShellContent
            ContentTemplate="{DataTemplate view:SettingsPage}"
            />
        </Tab>
    </TabBar>
</Shell>
